  name = Your Name
  email = your@email.com
[core]
  autocrlf = false
  safecrlf = true
  editor = vim
  autofrlf = input
[alias]
  co = checkout
  c = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[push]
  default = current
